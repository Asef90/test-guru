<h1>Test "<%= @test.title %>"</h1>
<%= link_to 'New question', new_test_question_path(@test) %>

<% @test.questions.each do |question| %>
  <p><%= question.body %></p>
  <%= link_to 'Show', question %>
  <%= link_to 'Edit', edit_question_path(question) %>
  <%= link_to 'Delete',
              question,
              method: :delete,
              data: { confirm: "Are tou sure?" } %>
<% end %>
