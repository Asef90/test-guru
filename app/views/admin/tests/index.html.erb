<div class="row">
  <div class="col col-md">
    <h1><%= t('.header') %></h1>

    <p>
      <%= link_to t('.new_test'), new_admin_test_path %><br>
      <%= link_to t('.all_gists'), admin_gists_path %>
    </p>

    <%= render 'shared/errors', resource: @test %>

    <table class="table table-striped text-center mt-5">
      <tr>
        <th scope="col" class="w-50"><%= t('.title') %></th>
        <th scope="col"><%= t('.level') %></th>
        <th scope="col" colspan="3"><%= t('.actions') %></th>
      </tr>

      <%= render @tests %>
    </table>
  </div>
</div>


